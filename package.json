{
  "name": "petitions-workspace",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "--------------------개발": "#",
    "next": "cd packages/ptt_frontend && AWS_PROFILE=deptno next",
    "graphql": "AWS_PROFILE=deptno tsnd packages/ptt_backend/handler/graphql.local.ts",
    "cron:petitions": "RUN=1 tsnd --respawn packages/ptt_datalake/cron/petitions.ts",
    "--------------------빌드": "#",
    "build": "yarn build:graphql",
    "build:graphql": "graphql-codegen --config codegen.yml",
    "dev:build:frontend": "cd packages/ptt_frontend && next build && next export && touch out/.nojekyll",
    "dev:package:backend": "cd packages/ptt_backend && sls package -s dev",
    "dev:package:datalake": "cd packages/ptt_datalake && sls package -s dev",
    "dev:deploy:backend": "cd packages/ptt_backend && sls deploy -s dev",
    "dev:deploy:datalake": "cd packages/ptt_datalake && sls deploy -s dev",
    "dev:deploy:datalake.f": "cd packages/ptt_datalake && sls deploy -s dev -f cronGetPetitionsTop15",
    "--------------------테스트": "#",
    "test": "echo 'no test'",
    "--------------------기타": "#",
    "ws:remove:serverless-webpack": "yarn remove serverless-webpack -W",
    "ws:add:serverless-webpack": "yarn add serverless-webpack -D -W",
    "--------------------훅": "#",
    "postinstall": "yarn build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@graphql-codegen/cli": "^1.12.1",
    "@graphql-codegen/introspection": "^1.12.1",
    "@graphql-codegen/typescript": "^1.12.1",
    "@graphql-codegen/typescript-operations": "^1.12.1",
    "@graphql-codegen/typescript-resolvers": "^1.12.1",
    "@types/aws-lambda": "^8.10.40",
    "@types/node": "^13.5.0",
    "@types/ramda": "^0.26.40",
    "@types/react": "^16.9.19",
    "apollo-server": "^2.9.16",
    "aws-sdk": "^2.610.0",
    "babel-loader": "^8.0.6",
    "serverless": "^1.61.3",
    "serverless-jetpack": "^0.8.1",
    "serverless-webpack": "^5.3.1",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5"
  }
}
